<%- include('../partials/header') %>

    <br><br><br><br>
    <div>
        <img id="profile-avatar" src="<%= user.avatar %>" alt="avatar">
        <span>
            <%= user.name %>
        </span>
    </div>
    <div>
        <% doodles.forEach(function(doodle) { %>
            <li class="feed">
                <a href="/users/<%= doodle._id%>">
                    <img class="feed-post" src="<%= doodle.img %>">
                </a>
            </li>
        <% }); %>
    </div>
    <a href="/doodles/new">Add doodle</a>

    <div>

    </div>



    <%- include('../partials/footer') %>