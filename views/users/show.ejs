<%- include('../partials/header') %>

    <br><br><br><br>
    <div>
        <img id="profile-avatar" src="<%= user.avatar %>" alt="avatar">
        <span id="user-name-view">
            <%= user.name %>
        </span>
    </div>
    <div>
        <% doodles.forEach(function(doodle) { %>
            <li class="feed">
                <a href="/users/<%= doodle._id%>">
                    <img class="feed-post" src="<%= doodle.img %>">
                </a>
            </li>
            <% }); %>
    </div>
    <br>
    <!-- <a class="submit-button" href="/doodles/new">Add doodle</a> -->



    <%- include('../partials/footer') %>